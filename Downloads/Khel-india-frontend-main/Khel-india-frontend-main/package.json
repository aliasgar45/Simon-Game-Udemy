{
  "name": "khel-webapp",
  "version": "0.0.1",
  "private": true,
  "author": "Sanket Dofe",
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">= 8"
  },
  "scripts": {
    "start": "cross-env REACT_APP_ENV=development react-scripts start",
    "build": "CI=false REACT_APP_ENV=production react-scripts build",
    "build:staging": "CI=false REACT_APP_ENV=development react-scripts build",
    "prepare": "husky install",
    "test": "react-scripts test --env=jsdom --passWithNoTests",
    "testWithoutWatch": "set CI=true react-scripts test --env=jsdom --passWithNoTests",
    "testWithCoverage": "set CI=true react-scripts test --env=jsdom --coverage --passWithNoTests",
    "lint": "eslint --fix ./src/**/*.{ts,tsx}",
    "purgeDependencies": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json && npm install",
    "deleteDependencies": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@material-tailwind/react": "^2.0.7",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "@mui/x-date-pickers": "^6.0.0",
    "@sentry/react": "^7.20.0",
    "@sentry/tracing": "^7.20.0",
    "autoprefixer": "^10.4.14",
    "dayjs": "^1.11.6",
    "firebase": "^9.9.3",
    "jotai": "^1.8.0",
    "mixpanel-browser": "^2.45.0",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.3.0",
    "react-virtuoso": "^2.18.0",
    "socket.io-client": "^4.5.1",
    "styled-components": "^6.0.5",
    "tailwindcss": "^3.3.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/mixpanel-browser": "^2.38.0",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
