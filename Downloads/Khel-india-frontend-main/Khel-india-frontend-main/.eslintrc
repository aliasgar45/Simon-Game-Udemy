{
  "extends": "react-app",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json",
    "tsconfigRootDir": "./"
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["prettier","@typescript-eslint/eslint-plugin"],
  "root": true,
  "rules": {
    "prettier/prettier": "warn"
  }
}
